  "source": "// SPDX-License-Identifier: MIT\npragma solidity 0.8.19;\n\ninterface IData {\n    struct Bond {\n        string isin;\n        string name;\n        string symbol;\n        address currency;\n        uint256 denomination;\n        uint256 issueVolume;\n        uint256 couponRate;\n        uint8 couponType;\n        uint256 couponFrequency;\n        uint8 dayCountBasis;\n        uint256 issueDate;\n        uint256 maturityDate;\n    }\n\n    struct Issuer {\n        address accountAddress;\n        string name;\n        string country;\n        string email;\n        string category;\n        string creditRating;\n        uint256 carbonCredit;\n        string logoURI;\n    }\n\n    struct Offer {\n        address investor;\n        uint256 principal;\n    }\n\n    struct CouponPaymentReceipt {\n        string issuerName;\n        uint256 issueVolume;\n        uint256 couponRate;\n        uint256 interestPaid;\n    }\n}",
